{
  "name": "esm-fe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 3600",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/esm-fe/stats.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "test:ci": "ng test --code-coverage --no-watch --browsers=ChromeHeadlessCI",
    "prepare": "husky install",
    "lint": "ng lint",
    "preinstall": "npx only-allow pnpm",
    "generate-api": "node ./swagger-api-codegen",
    "format": "npx prettier --write ."
  },
  "private": true,
